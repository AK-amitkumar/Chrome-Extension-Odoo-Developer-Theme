{
    "name": "Odoo Developer",
    "version": "0.1.1",
    "manifest_version": 2,
    "description": "Tools & helpers for Odoo developers",
    "homepage_url": "https://github.com/nedaszilinskas",
    "content_scripts": [{
        "matches": [
            "*://*/web*"
        ],
        "js": [
            "src/js/base.js"
        ],
        "all_frames": true,
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "src/js/odoo.js",
        "src/css/dark.css",
        "src/xml/base.xml"
    ]
}